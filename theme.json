{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": false,
		"color": {
			"palette": [
				{ "slug": "orange", "color": "#F9A134", "name": "Orange" },
				{ "slug": "dark-teal", "color": "#00A69C", "name": "Dark Teal" },
				{ "slug": "teal", "color": "#72CCD2", "name": "Teal" },
				{ "slug": "blue", "color": "#699CC6", "name": "Blue" },
				{ "slug": "dark-blue", "color": "#004261", "name": "Dark Blue" },
				{ "slug": "charcoal", "color": "#414141", "name": "Charcoal" },
				{ "slug": "off-white", "color": "#EDEDED", "name": "Off White" },
				{ "slug": "white", "color": "#FFFFFF", "name": "White" }
			],
			"gradients": [],
			"custom": false
		},
		"typography": {
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFamily": "'Raleway', 'Helvetica Neue', Helvetica, Arial, sans-serif",
					"slug": "raleway",
					"name": "Raleway"
				}
			],
			"fontSizes": [
				{ "slug": "small", "size": "0.875rem", "name": "Small" },
				{ "slug": "medium", "size": "1rem", "name": "Medium" },
				{ "slug": "large", "size": "1.25rem", "name": "Large" },
				{ "slug": "x-large", "size": "1.5rem", "name": "Extra Large" }
			]
		},
		"spacing": {
			"padding": false,
			"margin": false,
			"units": []
		},
		"layout": {
			"contentSize": "54rem",
			"wideSize": "72rem"
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "'Raleway', 'Helvetica Neue', Helvetica, Arial, sans-serif"
		},
		"spacing": {
			"blockGap": null
		}
	}
}
